{
  "model": {
    "type": "select",
    "property": "component",
    "component": "provides_banner_patterns",
    "cases": [
      {
        "when": "#academie:dormitories",
        "model": {
          "type": "model",
          "model": "herobrine:item/banner_pattern/dormitories"
        }
      },
      {
        "when": "#herobrine:villages",
        "model": {
          "type": "model",
          "model": "herobrine:item/banner_pattern/villages"
        }
      },
      {
        "when": "#pattern_item/globe",
        "model": {
          "type": "model",
          "model": "item/globe_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/flow",
        "model": {
          "type": "model",
          "model": "item/flow_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/bordure_indented",
        "model": {
          "type": "model",
          "model": "item/bordure_indented_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/guster",
        "model": {
          "type": "model",
          "model": "item/guster_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/field_masoned",
        "model": {
          "type": "model",
          "model": "item/field_masoned_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/mojang",
        "model": {
          "type": "model",
          "model": "item/mojang_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/skull",
        "model": {
          "type": "model",
          "model": "item/skull_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/creeper",
        "model": {
          "type": "model",
          "model": "item/creeper_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/piglin",
        "model": {
          "type": "model",
          "model": "item/piglin_banner_pattern"
        }
      },
      {
        "when": "#pattern_item/flower",
        "model": {
          "type": "model",
          "model": "item/flower_banner_pattern"
        }
      }
    ],
    "fallback": {
      "type": "model",
      "model": "herobrine:item/banner_pattern/blank"
    }
  }
}