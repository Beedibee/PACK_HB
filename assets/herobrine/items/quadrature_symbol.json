{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "side",
        "model": {
          "type": "minecraft:model",
          "model": "herobrine:item/quad_side"
        }
      },
      {
        "when": "corner",
        "model": {
          "type": "minecraft:model",
          "model": "herobrine:item/quad_corner"
        }
      }
    ],
    "fallback": {
      "type": "minecraft:model",
      "model": "herobrine:item/quad_center"
    }
  }
}