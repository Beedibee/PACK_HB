      {
        "when": "prestige",
        "model": {
          "type": "minecraft:select",
          "property": "minecraft:charge_type",
          "cases": [
            {
              "when": "arrow",
              "model": {
                "type": "minecraft:model",
                "model": "herobrine:item/erylum_arsenal/prestige_charged"
              }
            },
            {
              "when": "rocket",
              "model": {
                "type": "minecraft:model",
                "model": "herobrine:item/erylum_arsenal/prestige_charged"
              }
            }
